---
templatized: true
published: true
content_type: eventstype
title: Template of an event
---
{% extends index %}

{% block 'inner-page' %}

<div id="features">
  <div class="container">
    <h1>
      {{ eventstype.name }}
    </h1>
    <div>
      <div class="row-fluid feature">
        {{ eventstype.zone1 }}
      </div>
      <div class="row-fluid feature">
        {{ eventstype.zone2 }}
      </div>
      <div class="row-fluid feature">
        {{ eventstype.zone3 }}
      </div>
      {% if eventstype.logoreference1.url != nil and eventstype.logoreference2.url != nil and eventstype.logoreference3.url != nil %}
        <div class="row-fluid feature">
          <div class="span4 event-reference">
            {{ eventstype.logoreference1.url | image_tag }}<br><br>
            "{{ eventstype.quotereference1 }}"
          </div>
          <div class="span4 event-reference">
            {{ eventstype.logoreference2.url | image_tag }}<br><br>
            "{{ eventstype.quotereference2 }}"
          </div>
          <div class="span4 event-reference">
            {{ eventstype.logoreference3.url | image_tag }}<br><br>
            "{{ eventstype.quotereference3 }}"
          </div>
        </div>
      {% elsif eventstype.logoreference1.url != nil and eventstype.logoreference2.url != nil and eventstype.logoreference3.url == nil %}
        <div class="row-fluid feature">
          <div class="span6 event-reference">
            {{ eventstype.logoreference1.url | image_tag }}<br><br>
            "{{ eventstype.quotereference1 }}"
          </div>
          <div class="span6 event-reference">
            {{ eventstype.logoreference2.url | image_tag }}<br><br>
            "{{ eventstype.quotereference2 }}"
          </div>
        </div>
      {% elsif eventstype.logoreference1.url != nil and eventstype.logoreference2.url == nil and eventstype.logoreference3.url == nil %}
        <div class="row-fluid feature">
          <div class="span12 event-reference">
            {{ eventstype.logoreference1.url | image_tag }}<br><br>
            "{{ eventstype.quotereference1 }}"
          </div>
        </div>
      {% endif %}
      <div class="row-fluid feature">
        {{ eventstype.example }}
      </div>
    </div> 
    <div>
      <div id="quick-action" style="text-align: center">
        <a href="https://app.mobicheckin.com/en/users/sign_in#auto_demo_sign_in" class="btn btn-larger btn-quick-tour">
          {% editable_short_text "try_demo", hint: "try_demo", priority: 1, fixed: true %}
            Try Online Demo
          {% endeditable_short_text %}
        </a>
        <a href="https://app.mobicheckin.com/en/users/sign_up" class="btn btn-info btn-larger">
          {% editable_short_text "try_demo", hint: "try_demo", priority: 1, fixed: true %}
            Sign up for Free
          {% endeditable_short_text %}
        </a>
      </div>
    </div>
  </div>
</div> 

{% endblock %}